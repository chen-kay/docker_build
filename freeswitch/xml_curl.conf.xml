<configuration name="xml_curl.conf" description="cURL XML Gateway">
  <bindings>
    <binding name="directory">
        <param name="gateway-url" value="http://${domain}:${ip}/fs/" bindings="directory" />
    </binding>
    <binding name="dialplan">
        <param name="gateway-url" value="http://${domain}:${ip}/fs/" bindings="dialplan" />
    </binding>
    <binding name="configuration">
        <param name="gateway-url" value="http://${domain}:${ip}/fs/" bindings="configuration" />
    </binding>
  </bindings>
</configuration>